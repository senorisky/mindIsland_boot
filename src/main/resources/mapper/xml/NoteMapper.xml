<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lifemind.bluer.mapper.NoteMapper">

    <select id="selectNoteIdListByUid" resultType="java.lang.String">
        select n.id
        from note n
        where n.user_id = #{userId}
    </select>
</mapper>
